generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @id @default(autoincrement())
  password String  
  userName String @unique
}

model Hanzi {
  id     Int     @default(autoincrement()) @id
  chinese   String
  pinyin    String
  bihua     String
  bishun    String
  radical   String
  fayin     String?
}


model Chinese {
  id     Int     @default(autoincrement()) @id
  chinese   String
  pinyin    String
  bihua     String
  bishun    String
  radical   String
  fayin     String?
}
model ChineseStudy {
  id          Int      @default(autoincrement()) @id
  begineTime  DateTime @default(now())
  errorCounts Int
  finished    Boolean  @default(false)
}